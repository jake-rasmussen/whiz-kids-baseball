{
  "name": "whiz-kids",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker:up": "docker compose up --detach --no-recreate --wait",
    "docker:down": "docker compose down --timeout 5",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "prisma:push": "prisma db push",
    "prisma:dev": "yarn docker:up && dotenv -e .env -- prisma migrate dev",
    "prisma:dev:studio": "yarn docker:up && dotenv -e .env -- prisma studio",
    "prisma:dev:reset": "yarn docker:up && dotenv -e .env -- prisma migrate reset --force",
    "build": "next build",
    "dev": "yarn prisma:dev && next dev",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"{prisma,src}/**/*.{ts,tsx}\"",
    "eslint": "eslint \"./**/*.{ts,tsx}\" --max-warnings 0",
    "eslint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
    "cleanup": "yarn eslint:fix && npm run format",
    "postinstall": "yarn prisma:generate && husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed/index.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.8.2",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@prisma/client": "^4.8.0",
    "@tabler/icons": "^1.119.0",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.20.0",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "daisyui": "^2.51.2",
    "dayjs": "^1.11.7",
    "embla-carousel": "^7.0.9",
    "embla-carousel-autoplay": "^7.0.9",
    "embla-carousel-react": "^7.0.5",
    "next": "13.1.1",
    "next-videos": "^1.4.1",
    "nodemailer": "^6.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.9.1",
    "tw-elements": "^1.0.0-alpha13",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.8.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.0.0"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --check",
    "./**/*.{ts,tsx}": "eslint --max-warnings 0"
  }
}
